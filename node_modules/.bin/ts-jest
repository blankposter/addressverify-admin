#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/amirdev/Desktop/js/addressverify-admin/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.3_jest@30.1.3_typescript@5.9.2/node_modules/ts-jest/node_modules:/Users/amirdev/Desktop/js/addressverify-admin/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.3_jest@30.1.3_typescript@5.9.2/node_modules:/Users/amirdev/Desktop/js/addressverify-admin/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/amirdev/Desktop/js/addressverify-admin/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.3_jest@30.1.3_typescript@5.9.2/node_modules/ts-jest/node_modules:/Users/amirdev/Desktop/js/addressverify-admin/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.3_jest@30.1.3_typescript@5.9.2/node_modules:/Users/amirdev/Desktop/js/addressverify-admin/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
