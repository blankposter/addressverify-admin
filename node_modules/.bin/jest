#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/amirdev/Desktop/js/addressverify-admin/node_modules/.pnpm/jest@30.1.3_@types+node@22.18.1_ts-node@10.9.2/node_modules/jest/bin/node_modules:/Users/amirdev/Desktop/js/addressverify-admin/node_modules/.pnpm/jest@30.1.3_@types+node@22.18.1_ts-node@10.9.2/node_modules/jest/node_modules:/Users/amirdev/Desktop/js/addressverify-admin/node_modules/.pnpm/jest@30.1.3_@types+node@22.18.1_ts-node@10.9.2/node_modules:/Users/amirdev/Desktop/js/addressverify-admin/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/amirdev/Desktop/js/addressverify-admin/node_modules/.pnpm/jest@30.1.3_@types+node@22.18.1_ts-node@10.9.2/node_modules/jest/bin/node_modules:/Users/amirdev/Desktop/js/addressverify-admin/node_modules/.pnpm/jest@30.1.3_@types+node@22.18.1_ts-node@10.9.2/node_modules/jest/node_modules:/Users/amirdev/Desktop/js/addressverify-admin/node_modules/.pnpm/jest@30.1.3_@types+node@22.18.1_ts-node@10.9.2/node_modules:/Users/amirdev/Desktop/js/addressverify-admin/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jest/bin/jest.js" "$@"
else
  exec node  "$basedir/../jest/bin/jest.js" "$@"
fi
