#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/amirdev/Desktop/js/addressverify-admin/node_modules/@angular-devkit/schematics-cli/bin/node_modules:/Users/amirdev/Desktop/js/addressverify-admin/node_modules/@angular-devkit/schematics-cli/node_modules:/Users/amirdev/Desktop/js/addressverify-admin/node_modules/@angular-devkit/node_modules:/Users/amirdev/Desktop/js/addressverify-admin/node_modules:/Users/amirdev/Desktop/js/node_modules:/Users/amirdev/Desktop/node_modules:/Users/amirdev/node_modules:/Users/node_modules:/node_modules:/Users/amirdev/Desktop/js/addressverify-admin/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/amirdev/Desktop/js/addressverify-admin/node_modules/@angular-devkit/schematics-cli/bin/node_modules:/Users/amirdev/Desktop/js/addressverify-admin/node_modules/@angular-devkit/schematics-cli/node_modules:/Users/amirdev/Desktop/js/addressverify-admin/node_modules/@angular-devkit/node_modules:/Users/amirdev/Desktop/js/addressverify-admin/node_modules:/Users/amirdev/Desktop/js/node_modules:/Users/amirdev/Desktop/node_modules:/Users/amirdev/node_modules:/Users/node_modules:/node_modules:/Users/amirdev/Desktop/js/addressverify-admin/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@angular-devkit/schematics-cli/bin/schematics.js" "$@"
else
  exec node  "$basedir/../@angular-devkit/schematics-cli/bin/schematics.js" "$@"
fi
